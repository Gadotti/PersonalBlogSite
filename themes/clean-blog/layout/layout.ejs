<!DOCTYPE html>
<html <% if(config.language) { %>lang="<%= config.language %>"<% } else { %>lang="en"<% }%>>

<!-- Head tag -->
<%- partial('_partial/head') %>

<body>

    <!-- Menu -->
    <%- partial('_partial/menu') %>

    <!-- Main Content -->
    <%- body %>

    <!-- Google InArtcileAd -->
    <%- partial('googleads/inarticlead') %>

    <!-- Footer -->
    <%- partial('_partial/footer') %>

    <a id="rocket" href="#top" class="">
        <svg fill="rgb(64, 64, 64)" id="Icons" style="enable-background:new 0 0 32 32;" version="1.1" viewBox="0 0 32 32" xml:space="preserve" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink"><g><path d="M24.4,24c-0.9,0-1.8-0.3-2.5-1l-5.8-5.7c-0.1-0.1-0.2-0.1-0.3,0L10.1,23c-1.4,1.4-3.6,1.4-5,0c-0.7-0.7-1-1.6-1-2.5   c0-1,0.4-1.8,1-2.5l9.4-9.3c0.9-0.9,2.3-0.9,3.1,0l9.4,9.3c0.7,0.7,1,1.6,1,2.5c0,1-0.4,1.8-1,2.5C26.2,23.7,25.3,24,24.4,24z"/></g></svg>
    </a>
    <script type="text/javascript" src="/js/totop.js?v=1.0.0" async=""></script>
    
    <script type="text/javascript" src="/js/three.min.js?v=1.0.0"></script>
    <script type="text/javascript" src="/js/vanta.birds.min.js?v=1.0.0"></script>
    <script type="text/javascript" src="/js/vanta.globe.min.js?v=1.0.0"></script>
    <script type="text/javascript" src="/js/vanta.net.min.js?v=1.0.0"></script>
    <script type="text/javascript" src="/js/vanta.halo.min.js?v=1.0.0"></script>
    <script>
        const vantasDivID = '#my-vantas';
        let backgroundType = 1;

        function changeMe() {
            if (backgroundType > 4) {
                backgroundType = 1;
            }

            switch (backgroundType) {
                case 1:
                    VANTA.BIRDS({
                        el: vantasDivID,
                        mouseControls: true,
                        touchControls: true,
                        gyroControls: false,
                        minHeight: 200.00,
                        minWidth: 200.00,
                        scale: 1.00,
                        scaleMobile: 1.00
                    });
                    break;
                case 2:
                    VANTA.GLOBE({
                        el: vantasDivID,
                        mouseControls: true,
                        touchControls: true,
                        gyroControls: false,
                        minHeight: 200.00,
                        minWidth: 200.00,
                        scale: 1.00,
                        scaleMobile: 1.00
                    });
                    break;
                case 3:
                    VANTA.NET({
                        el: vantasDivID,
                        mouseControls: true,
                        touchControls: true,
                        gyroControls: false,
                        minHeight: 200.00,
                        minWidth: 200.00,
                        scale: 1.00,
                        scaleMobile: 1.00
                    });
                    break;
                case 4:
                    VANTA.HALO({
                        el: vantasDivID,
                        mouseControls: true,
                        touchControls: true,
                        gyroControls: false,
                        minHeight: 200.00,
                        minWidth: 200.00
                    });
                    break;
            }

            backgroundType++;
        }

        changeMe();        
    </script>

    <!-- After footer scripts -->
    <%- partial('_partial/after-footer') %>

</body>

</html>